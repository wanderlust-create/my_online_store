<%= form_with url: merchant_items_path(@merchant.id), method: :post, local: true do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :description %>
<%= f.text_field :description %>

<%= f.label :unit_price %>
<%= f.number_field :unit_price, step: :any %>

<%= f.submit 'Submit' %>
<% end %>
