<h1>All Merchants:</h1>
<% @merchants.each_with_index do |merchant, index| %>
<div id="merchant-<%= index %>">
  <h4>Merchant Name: <%= merchant.name %></h4>
  <%= link_to "#{merchant.name}'s items list", merchant_items_path(merchant) %><br>
  <%= link_to "#{merchant.name}'s shipments list", merchant_shipments_path(merchant) %><br><br>
</div>
<% end %>
